{
  "name": "juice-sdk-core",
  "version": "8.0.1-alpha",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "pregenerate": "ts-node ./scripts/generateAddresses.ts",
    "generate": "wagmi generate",
    "prebuild": "rm -Rf dist/",
    "build": "yarn generate && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --project ./tsconfig.json",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "dev": "tsc --project ./tsconfig.json --watch --preserveWatchOutput"
  },
  "dependencies": {
    "bs58": "^5.0.0",
    "fpnum": "^1.0.0"
  },
  "peerDependencies": {
    "viem": "^1.21.4"
  },
  "devDependencies": {
    "@bananapus/721-hook": "0.0.12",
    "@bananapus/core": "0.0.14",
    "@types/bs58": "^4.0.4",
    "@wagmi/cli": "^1.5.2",
    "patch-package": "^8.0.0",
    "typescript": "^5.2.2",
    "viem": "^1.21.4"
  }
}
