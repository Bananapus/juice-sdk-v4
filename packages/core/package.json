{
  "name": "juice-sdk-core",
  "version": "1.7.2-beta",
  "type": "module",
  "scripts": {
    "generate": "wagmi generate",
    "postgenerate": "node ./scripts/addJBProjectDeploymentAddresses.js",
    "prebuild": "rm -Rf dist/",
    "build": "npm run generate && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project ./tsconfig.json",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "dev": "tsc --project ./tsconfig.json --watch --preserveWatchOutput",
    "test": "vitest run"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/esm/index.d.ts",
  "dependencies": {
    "bs58": "^5.0.0",
    "fpnum": "^1.0.0"
  },
  "devDependencies": {
    "@bananapus/721-hook": "^1.0.0",
    "@bananapus/address-registry": "^1.0.0",
    "@bananapus/buyback-hook": "^1.0.0",
    "@bananapus/core": "^1.0.2",
    "@bananapus/suckers": "^1.0.2",
    "@bananapus/swap-terminal": "^1.0.0",
    "@bananapus/omnichain-deployers": "github:bananapus/nana-omnichain-deployers#751451863788bc59f995282ef6e889154b592cde",
    "@jbm/wagmi-config": "*",
    "@wagmi/cli": "^2.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "viem": "^2.12.0",
    "vitest": "^1.6.0",
    "wagmi": "^2.10.2"
  },
  "engines": {
    "node": "20.x"
  }
}
